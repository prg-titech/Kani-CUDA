package Expression;
public class Variable extends AExpression{
	String name;

	public Variable(String name) {
		super();
		this.name = name;
	}
	
	public String getName(){
		return this.name;
	}
	
	public String toStringExp(){
		return this.name;
	}

	public int eval(Environment env){
		return env.getVal(this);
	}
	
	public boolean isVariable(){
		return true;
	}
}
